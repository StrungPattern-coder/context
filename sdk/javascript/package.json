{
  "name": "ral-sdk",
  "version": "0.1.0",
  "description": "RAL SDK - Context-aware AI augmentation for any AI provider",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "context",
    "augmentation",
    "llm",
    "openai",
    "anthropic",
    "claude",
    "gemini",
    "ral"
  ],
  "author": "RAL Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ral-dev/ral"
  },
  "homepage": "https://docs.ral.dev",
  "bugs": {
    "url": "https://github.com/ral-dev/ral/issues"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "sideEffects": false
}
